[
  {
    "name": "admin",
    "image": "${admin-docker-image}",
    "cpu": 10,
    "memory": 512,
    "links": [],
    "hostname": "admin",
    "portMappings": [
      {
        "containerPort": 50055,
        "protocol": "tcp"
      }
    ],
    "essential": true,
    "entryPoint": [],
    "command": [],
    "environment": [
      {
        "name": "CONNECTOR_PSQL_HOST",
        "value": "${connector-psql-host}"
      },
      {
        "name": "CONNECTOR_PSQL_DATABASE",
        "value": "${connector-psql-database}"
      },
      {
        "name": "CONNECTOR_PSQL_USERNAME",
        "value": "${connector-psql-username}"
      },
      {
        "name": "CONNECTOR_PSQL_PASSWORD",
        "value": "${connector-psql-password}"
      }
    ],
    "mountPoints": [],
    "volumesFrom": [],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${awslogs-group}",
        "awslogs-region": "${awslogs-region}"
      }
    },
    "networkMode": "bridge"
  }
]