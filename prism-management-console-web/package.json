{
  "name": "prism-management-console-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prestart": "./scripts/compile-protos.sh",
    "start": "./scripts/copyEnvVarsToWindow.sh && cp env-config.js ./public/ && react-scripts start",
    "prestart-test-server": "./scripts/compile-protos.sh",
    "start-test-server": "cp ./cypress/App.js ./src/ && react-scripts start",
    "prebuild": "./scripts/compile-protos.sh",
    "build": "react-scripts build",
    "pretest": "./scripts/compile-protos.sh",
    "test": "react-scripts test --testPathIgnorePatterns=src/__tests__/__mocks__ --testResultsProcessor jest-sonar-reporter --coverage .",
    "e2e-test": "npx cypress run",
    "pretest-no-watch": "./scripts/compile-protos.sh",
    "test-no-watch": "react-scripts test --watchAll=false --testPathIgnorePatterns=src/__tests__/__mocks__",
    "eject": "react-scripts eject",
    "eslint": "npx eslint . --ext .jsx --ext .js",
    "ci-e2e": "start-server-and-test start-test-server http://localhost:3000 e2e-test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache"
  },
  "dependencies": {
    "@ant-design/compatible": "1.0.0",
    "@ant-design/icons": "4.4.0",
    "antd": "4.10.2",
    "base64-js": "1.3.1",
    "base64url": "3.0.1",
    "core-js": "3.6.5",
    "env-cmd": "10.0.1",
    "faker": "4.1.0",
    "file-saver": "2.0.2",
    "get-contrast": "3.0.0",
    "google-protobuf": "3.9.2",
    "grpc-web": "1.0.7",
    "i18next": "17.0.18",
    "jschardet": "2.2.1",
    "lodash": "4.17.21",
    "mobx": "6.3.2",
    "mobx-react-lite": "3.2.0",
    "mobx-utils": "6.0.4",
    "moment": "2.24.0",
    "node-sass": "6.0.0",
    "papaparse": "5.3.0",
    "prop-types": "15.7.2",
    "qrcode.react": "0.9.3",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-i18next": "10.13.1",
    "react-json-view": "1.19.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "4.0.3",
    "react-sortable-hoc": "2.0.0",
    "react-spinners": "0.6.1",
    "react-tooltip": "3.11.2",
    "sanitize-html": "2.1.1",
    "urlsafe-base64": "1.0.0",
    "uuid": "7.0.3"
  },
  "devDependencies": {
    "@input-output-hk/atala-prism-protos": "0.1.0-ff1968f",
    "@testing-library/react": "9.4.0",
    "babel-eslint": "^10.1.0",
    "cypress": "4.12.1",
    "env-cmd": "10.0.1",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-meteor": "0.4.0",
    "eslint-plugin-chai-friendly": "0.4.1",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-cypress": "2.7.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.2",
    "jest-sonar-reporter": "2.0.0",
    "prettier": "1.16.1",
    "react-app-rewired": "2.1.3",
    "start-server-and-test": "1.10.6"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "testlog.xml"
  }
}
